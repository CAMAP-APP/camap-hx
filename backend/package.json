{
  "name": "camap-haxe-backend",
  "description": "",
  "author": "Fran√ßois Barbut <francois@alilo.fr>",
  "license": "GPL-2.0",
  "homepage": "https://github.com/camap-app",
  "devDependencies": {
    "@types/react": "^16.9.55",
    "npm-run-all": "^4.1.2"
  },
  "scripts": {
    "build:hx": "haxe build.hxml",
    "build:tmpl": "neko temploc2.n -macros ../lang/master/tpl/macros.mtt -output ../lang/master/tmp/ ../lang/master/tpl/*.mtt ../lang/master/tpl/*/*.mtt ../lang/master/tpl/*/*/*.mtt",
    "build:sass": "./buildSass.sh",
    "dev:sass": "sass ../www/theme",
    "watch": "haxe-watchify --program haxe --hxml build.hxml --src ../src --src ../js --src ../common",
    "i18n:extract": "mv ../www/lang/allTexts.pot ../www/lang/allTexts.pot.bak && haxe potGeneration.hxml && npm run i18n:diff -- ../www/lang/allTexts.pot.bak ../www/lang/allTexts.pot",
    "i18n:diff": "node i18n.js diff",
    "i18n:translate": "node i18n.js update ../www/lang/allTexts.pot ../www/lang/texts_fr.po",
    "i18n:compile": "node i18n.js compile"
  },
  "dependencies": {
    "gettext-parser": "^8.0.0",
    "haxe-watchify": "^1.3.0",
    "sass": "^1.92.1"
  }
}
